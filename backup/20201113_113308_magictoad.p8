--messages

function init_msg()
	messages={}
end

function create_msg(name,...)
	msg_title=name
	messages={...}
end

function update_msg()
	if (btnp(❎)) then
		deli(messages,1)
	
	end
end	
		

function draw_msg()
local y=85
	if messages[1] then
	print(msg_title,0,y,3)
	print (messages[1],0,y+10,3)
	end
end	 	
