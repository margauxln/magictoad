function box_hit(
  x1,y1,
  w1,h1,
  x2,y2,
  w2,h2)
    
  hit=false
  local xd=abs((x1+(w1/2))-(x2+(w2/2)))
  local xs=w1*0.5+w2*0.5
  local yd=abs((y1+(h1/2))-(y2+(h2/2)))
  local ys=h1/2+h2/2
  if xd<xs and 
     yd<ys then 
    hit=true 
  end  
  
  return hit
end

function collision(x,y)

collision=false
if (btn(1)) then
if fget(mget(flr((x+9)/8),
																		y_grid),0) then
collision=true
	end
end	

if (btn(0)) then
if fget(mget(flr((x-9)/8),
																		y_grid),0) then
collision=true		
			end
end		
	
if (btn(2)) then
if fget(mget(x_grid,flr((y-1)/8)),0) then

collision=true		
			end
end					 

if (btn(3)) then
if fget(mget(x_grid,(y+8)/8),0) then

collision=true		
			end
end		

return collision
end
