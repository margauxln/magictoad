function _init()
x=45
y=56
w=1
h=1

cam_x=0
cam_y=0

newx=x
newy=y

x_grid=flr(x/8)
y_grid=flr(y/8)

init_msg()

create_msg("karen",
"tu vas sortir\nhabillee comme ca ?!","c'est pas un peu court?!")
end

function _update()

	if not messages[1] then
	player_movement()
	update_msg()

end
end

function _draw()
cls()
if x>60 then cam_x=x-40 else cam_x=0
end
camera(cam_x,cam_y)
map(0,0,0,0)
spr(1,x,y)

draw_msg()

update_msg()

end
