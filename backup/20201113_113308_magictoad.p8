--player_movement

function player_movement()

newx=x
newy=y

if (btn(0) and x>0) then newx-=1 end
if (btn(1) and x>0)	then newx+=1 end
if (btn(3) and y<82) then newy+=1 end
if (btn(2) and y>0) then newy-=1  end

if not
check_collision(newx,newy)
then
x=newx
y=newy

end 
end


